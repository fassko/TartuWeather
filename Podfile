# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'TartuWeather' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'Fuzi'
  pod 'AlamoFuzi'
  pod 'Fabric'
  pod 'Crashlytics'
end

target 'TartuWeatherWidget' do
  use_frameworks!
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'Fuzi'
  pod 'AlamoFuzi'
  pod 'Fabric'
  pod 'Crashlytics'
end

target 'meteoTartuUITests' do
  use_frameworks!
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'Fuzi'
  pod 'AlamoFuzi'
  pod 'Fabric'
  pod 'Crashlytics'
end

target 'meteoTartuUnitTests' do
  use_frameworks!
  
  pod 'Alamofire'
  pod 'AlamofireImage'
  pod 'Fuzi'
  pod 'AlamoFuzi'
  
